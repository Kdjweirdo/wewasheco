<p id="notice"><%= notice %></p>

<h1>Tableaus</h1>

<table>
  <thead>
    <tr>
      <th>Entreprise</th>
      <th>String</th>
      <th>Nom</th>
      <th>Email</th>
      <th>Numero</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tableaus.each do |tableau| %>
      <tr>
        <td><%= tableau.entreprise %></td>
        <td><%= tableau.string %></td>
        <td><%= tableau.nom %></td>
        <td><%= tableau.email %></td>
        <td><%= tableau.numero %></td>
        <td><%= link_to 'Show', tableau %></td>
        <td><%= link_to 'Edit', edit_tableau_path(tableau) %></td>
        <td><%= link_to 'Destroy', tableau, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tableau', new_tableau_path %>
